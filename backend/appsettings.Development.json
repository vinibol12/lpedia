{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "Logs/app.log",
      "RollingInterval": "Day"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=FurniFit.db",
    "AzureStorage": "UseDevelopmentStorage=true"
  },
  "OpenAI": {
    "ApiKey": ""
  }
}
